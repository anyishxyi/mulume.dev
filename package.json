{
  "name": "mulume",
  "description": "My personal website",
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "build": "ng build",
    "ci": "yarn clean-dist && yarn clean-modules && yarn install --frozen-lockfile",
    "clean": "yarn clean-dist && yarn clean-modules && yarn clean-nx && rm yarn.lock",
    "clean-dist": "rm -rf dist",
    "clean-modules": "rm -rf node_modules",
    "clean-nx": "rm -rf .nx/",
    "dev": "ng serve",
    "pre-commit": "yarn lint && yarn prettier && yarn test:coverage && yarn build",
    "prettier": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "test": "vitest run",
    "test:dev": "vitest dev --reporter=basic --disable-console-intercept",
    "test:coverage": "vitest run --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pxradox/mulume.dev"
  },
  "author": "jeanpaulngalula07@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/pxradox/mulume.dev",
  "dependencies": {
    "@analogjs/content": "^1.2.2",
    "@analogjs/router": "^1.2.2",
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "17.3.6",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@nx/angular": "~18.0.0",
    "front-matter": "^4.0.2",
    "googleapis": "^135.0.0",
    "marked": "^5.0.2",
    "marked-gfm-heading-id": "^3.1.0",
    "marked-highlight": "^2.0.1",
    "mermaid": "^10.2.4",
    "nodemailer": "^6.9.13",
    "prismjs": "^1.29.0",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.2.2",
    "@analogjs/vite-plugin-angular": "^1.2.2",
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@nx/vite": "~18.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/prismjs": "^1.26.3",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitest/coverage-v8": "^1.5.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.11",
    "jsdom": "^22.1.0",
    "nx": "~18.0.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=20.5.0"
  }
}
