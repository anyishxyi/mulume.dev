{
  "name": "mulume",
  "description": "My personal website",
  "version": "3.0.0",
  "scripts": {
    "build": "ng build --configuration=production",
    "clean-all": "npm run clean-dist && npm run clean-modules && rm package-lock.json",
    "clean-dist": "rimraf dist/*",
    "clean-modules": "rimraf node_modules/*",
    "dev": "ng serve",
    "i18n": "ng extract-i18n --output-path src/locale",
    "prettier": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "start:fr": "ng serve --configuration=fr",
    "start:prerender": "ng run mulume:prerender",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pxradox/mulume.dev"
  },
  "author": "jeanpaulngalula07@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/pxradox/mulume.dev",
  "dependencies": {
    "@angular/animations": "^16.2.11",
    "@angular/cdk": "^16.2.10",
    "@angular/common": "^16.2.11",
    "@angular/compiler": "^16.2.11",
    "@angular/core": "^16.2.11",
    "@angular/forms": "^16.2.11",
    "@angular/platform-browser": "^16.2.11",
    "@angular/platform-browser-dynamic": "^16.2.11",
    "@angular/router": "^16.2.11",
    "@types/nodemailer": "^6.4.9",
    "node-mailjet": "^6.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.8",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.8",
    "@angular/compiler-cli": "^16.2.11",
    "@angular/localize": "^16.2.11",
    "@inrupt/jest-jsdom-polyfills": "^2.5.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.17.6",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint",
      "pre-push": "yarn test"
    }
  },
  "engines": {
    "node": ">=18.16.1"
  }
}
